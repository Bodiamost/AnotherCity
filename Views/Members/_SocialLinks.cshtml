@model IEnumerable<AnotherCity.Models.MemberSocials>

@foreach (var social in Model)
{
        @if (social.SocialId == 1)
        {
            <dt><i class="fa fa-facebook"></i>Facebook</dt><dd>@Html.DisplayFor(itemSocial => social.PersonalLink)</dd>
        }
        else if (social.SocialId == 2)
        {
             <dt><i class="fa fa-google-plus"></i>Google</dt><dd>@Html.DisplayFor(itemSocial => social.PersonalLink)</dd>
        }
        else if (social.SocialId == 3)
        {
             <dt><i class="fa fa-linkedin"></i>LinkedIn</dt><dd>@Html.DisplayFor(itemSocial => social.PersonalLink)</dd>
        }
}